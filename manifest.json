{
	"name": "JSON Finder",
	"version": "0.0.4",
	"manifest_version": 2,
	"description": "Browse JSON like you do it in Finder.",
	"icons": { 
		"16": "icon16.png",
		"48": "icon48.png",
		"128": "icon128.png" },
	"content_scripts": [{
		"matches": ["<all_urls>"],
		"js": ["content.js"],
		"run_at" : "document_start"
	}],
	"homepage_url": "https://github.com/rapee/jsonfinder",
	"web_accessible_resources": ["style.css"]
}
